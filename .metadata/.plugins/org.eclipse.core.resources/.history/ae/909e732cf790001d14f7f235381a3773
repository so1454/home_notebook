package 일차원배열;

public class Ex05배열실습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] array = {9,1,8,72,31,55,66};
		
		//가장 큰 값을 출력하는 문제
		//예시 ) 가장 큰 값은 몇입니다.
		System.out.println("가장 큰 값은 ");
		int max = 0;
		for(int i = 0; i < array.length ; i++) {
			
			if (max < array[i]) {
				max = array[i];
				System.out.print(array[i]);
			}
		}
		
		
		
		
		
	}

}
